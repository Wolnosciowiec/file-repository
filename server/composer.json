{
    "name": "riotkit/backup-repository",
    "type": "project",
    "description": "Specialized ninja for backup storage. Designed to have very low resource requirements.",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "aws/aws-sdk-php": "^3.87",
        "coraxster/flysystem-aws-s3-v3-minio": "^1.0",
        "doctrine/annotations": "^1.0",
        "gabrielelana/byte-units": "^0.5.0",
        "glenscott/url-normalizer": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "lexik/jwt-authentication-bundle": "^2.8",
        "nelmio/api-doc-bundle": "^3.4",
        "oneup/flysystem-bundle": "^3.0",
        "oro/doctrine-extensions": "^1.3",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^1.1",
        "psr/simple-cache": "^1.0",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.2",
        "sentry/sentry-symfony": "^3.1",
        "symfony/asset": "4.4.*",
        "symfony/console": "*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.9",
        "symfony/framework-bundle": "*",
        "symfony/messenger": "4.4.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/security": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "*",
        "symfony/yaml": "*",
        "twistor/flysystem-stream-wrapper": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "^3.0",
        "flow/jsonpath": "^0.4.0",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "=8.2.4",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "*",
        "symfony/debug-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": ["vendor/mockery/mockery/library/helpers.php"]
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "update-scripts": [
            "./bin/console cache:clear",
            "./bin/console assets:install"
        ],
        "post-install-cmd": [
            "@update-scripts"
        ],
        "post-update-cmd": [
            "@update-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
